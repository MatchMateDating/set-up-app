{
  "expo": {
    "name": "matchmaker-mobile",
    "slug": "matchmaker-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "1b2bd6ca-592d-48d6-8e37-9498d29fb228"
      }
    },
    "splash": {
      "image": "./assets/splash-icon.png",
